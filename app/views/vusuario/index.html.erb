<p id="notice"><%= notice %></p>

<h1>Listing Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>nomusuario</th>
      <th>Email</th>
      <th>rut</th>
      <th>encrypted_password</th>
      <th>cargo</th>
      <th>estado</th>
      <th colspan="6"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |usuario| %>
      <tr>
        <td><%= user.nomusuario %></td>
        <td><%= user.email %></td>
        <td><%= user.rut %></td>
        <td><%= user.encrypted_password %></td>
        <td><%= user.cargo %></td>
        <td><%= user.estado %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>