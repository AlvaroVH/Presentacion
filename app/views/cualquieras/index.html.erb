<p id="notice"><%= notice %></p>

<h1>Listing Cualquieras</h1>

<table>
  <thead>
    <tr>
      <th>Columna1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cualquieras.each do |cualquiera| %>
      <tr>
        <td><%= cualquiera.columna1 %></td>
        <td><%= link_to 'Show', cualquiera %></td>
        <td><%= link_to 'Edit', edit_cualquiera_path(cualquiera) %></td>
        <td><%= link_to 'Destroy', cualquiera, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cualquiera', new_cualquiera_path %>
